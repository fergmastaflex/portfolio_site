<div class="page-header">
  <h1 class='big-header'><%= @project.title %>.</h1>
</div>
<div class="row">
  <div class="col-md-6">
    <% @project.pictures.each do |p| %>
      <%= image_tag p.image.url(:large), class: 'img-responsive' %>
    <% end %>
  </div>
  <div class='col-md-6'>
    <p><%= @project.description %></p>
  </div>
</div>